<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.collections.*?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
    xmlns:fx="http://javafx.com/fxml"
    fx:controller="sams.FXMLSummaryController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding> 
    
    <TextArea fx:id="sum" GridPane.columnIndex="0" GridPane.rowIndex="0"
    GridPane.columnSpan="10" GridPane.rowSpan="10"/> 
    <Button fx:id="submit" text="Submit" GridPane.columnIndex="5" GridPane.rowIndex="11" onAction="#handleButtonSubmit"</GridPane>
